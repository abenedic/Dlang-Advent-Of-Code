module main;
import std.stdio;

long paren_counter(string list)
body
{
    int accum = 0;
    int i = 0;
    foreach (char a; list)
    {
        if (accum == -1)
        {
            writeln(i);
        }
        if (a == '(')
        {
            accum++;
        }
        else if (a == ')')
        {
            accum--;
        }
        i++;
    }
    return accum;

}

unittest
{
    assert(paren_counter("()") == 0);
    assert(paren_counter("))(((((") == 3);

}

