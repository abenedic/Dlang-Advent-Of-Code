bool get_on(const ref bool[100][100] grid, int i, int j)
{
    if ((i == 0 && j == 0) || (i == 99 && j == 0) || (i == 0 && j == 99) || (i == 99 && j == 99))
    {
        return true;
    }
    return grid[i][j];
}

int get_neighbor_on_count(const ref bool[100][100] grid, int i, int j)
{
    int on(bool a)
    {
        return a ? 1 : 0;
    }

    int count = 0;
    if (i != 0)
    {
        if (j != 0)
        {
            count += on(get_on(grid, i - 1, j - 1)); //fix the rest.
        }
        count += on(get_on(grid, i - 1, j));
        if (j != 99)
        {
            count += on(get_on(grid, i - 1, j + 1));
        }
    }

    if (j != 0)
    {
        count += on(get_on(grid, i, j - 1)); //fix the rest.
    }
    if (j != 99)
    {
        count += on(get_on(grid, i, j + 1));
    }
    if (i != 99)
    {
        if (j != 0)
        {
            count += on(get_on(grid, i + 1, j - 1)); //fix the rest.
        }
        count += on(get_on(grid, i + 1, j));
        if (j != 99)
        {
            count += on(get_on(grid, i + 1, j + 1));
        }
    }

    return count;

}

bool should_be_on(const ref bool[100][100] grid, int i, int j)
{
    int count = get_neighbor_on_count(grid, i, j);
    if (grid[i][j])
    {
        return count == 2 || count == 3;
    }
    return count == 3;
}

bool[100][100] get_next_grid(const ref bool[100][100] grid)
{
    bool[100][100] next_grid;
    for (int i = 0; i < 100; i++)
    {
        for (int j = 0; j < 100; j++)
        {
            next_grid[i][j] = should_be_on(grid, i, j);
        }
    }
    next_grid[0][0] = true;
    next_grid[99][0] = true;

    next_grid[0][99] = true;

    next_grid[99][99] = true;

    return next_grid;

}

int count_lights(const ref bool[100][100] grid)
{
    int count = 0;
    foreach (a; grid)
    {
        foreach (b; a)
        {
            if (b)
            {
                count++;
            }
        }
    }
    return count;
}

void main()
{
    bool[100][100] grid;
    int line = 0;
    int col = 0;
    foreach (s; input.splitLines)
    {
        foreach (c; s)
        {
            grid[line][col++] = c == '#' ? true : false;
        }
        line++;
        col = 0;
    }

    foreach (i; 0 .. 100)
    {
        grid = get_next_grid(grid);
    }
    auto c = count_lights(grid);
    writeln(c);
    if (c <= 360)
    {
        writeln("warning too low!");
    }
}

